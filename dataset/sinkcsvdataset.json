{
	"name": "sinkcsvdataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageLS",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "demofile.csv",
				"folderPath": {
					"value": "@concat('file', utcNow('yyyy-MM-DD')) ",
					"type": "Expression"
				},
				"fileSystem": "democontainer"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}