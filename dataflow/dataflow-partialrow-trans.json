{
	"name": "dataflow-partialrow-trans",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dataset_title_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TITLE_ID as integer,",
				"          { TITLE} as string,",
				"          { JOB_DIVISION} as string,",
				"          { JOB_FAMILY} as string,",
				"          { JOB_TITLE} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive({ TITLE} = split({ TITLE}, '-'),",
				"          TITLE_ID = TITLE_ID) ~> derivedColumn1",
				"derivedColumn1 foldDown(unroll( TITLE),",
				"     mapColumn(",
				"          TITLE_ID,",
				"          { TITLE},",
				"          { JOB_DIVISION},",
				"          { JOB_FAMILY},",
				"          { JOB_TITLE}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> flatten1"
			]
		}
	}
}