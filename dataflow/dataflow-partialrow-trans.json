{
	"name": "dataflow-partialrow-trans",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dataset_title_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "Dataset_csv_copytitle",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Dataset_sink_title_csv",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "join1"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TITLE_ID as integer,",
				"          { TITLE} as string,",
				"          { JOB_DIVISION} as string,",
				"          { JOB_FAMILY} as string,",
				"          { JOB_TITLE} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          EMP_ID as string,",
				"          EMPLOYEE as string,",
				"          EMP_DEPT as string,",
				"          { JOB_TITLE} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"select1 derive({ TITLE} = split({ TITLE}, '-'),",
				"          TITLE_ID = TITLE_ID) ~> derivedColumn1",
				"source1, source2 join({ JOB_FAMILY} == EMP_DEPT,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          TITLE_ID,",
				"          { TITLE},",
				"          { JOB_DIVISION},",
				"          { JOB_TITLE} = source1@{ JOB_TITLE},",
				"          EMP_ID,",
				"          EMPLOYEE,",
				"          EMP_DEPT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"derivedColumn1 derive({ TITLE} = { TITLE}[1]) ~> derivedColumn2",
				"derivedColumn2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          dept as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}