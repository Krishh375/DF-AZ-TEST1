{
	"name": "DF_EXT_REPORTE_SZRPPIN_CAMPUS_CONGELADOS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DF_EXT_REPORTE_SZRPPIN_CAMPUS_SOURCE_DATASET",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DF_EXT_REPORTE_SZRPPIN_CAMPUS_SINK_DATASET",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "sort1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     id as integer[] ([842302,842517,84300903,844359,46])",
				"}",
				"source(output(",
				"          id as integer,",
				"          diagnosis as string,",
				"          radius_mean as string,",
				"          texture_mean as string,",
				"          perimeter_mean as string,",
				"          area_mean as string,",
				"          smoothness_mean as string,",
				"          compactness_mean as string,",
				"          concavity_mean as string,",
				"          {concave points_mean} as string,",
				"          symmetry_mean as string,",
				"          fractal_dimension_mean as string,",
				"          radius_se as string,",
				"          texture_se as string,",
				"          perimeter_se as string,",
				"          area_se as string,",
				"          smoothness_se as string,",
				"          compactness_se as string,",
				"          concavity_se as string,",
				"          {concave points_se} as string,",
				"          symmetry_se as string,",
				"          fractal_dimension_se as string,",
				"          radius_worst as string,",
				"          texture_worst as string,",
				"          perimeter_worst as string,",
				"          area_worst as string,",
				"          smoothness_worst as string,",
				"          compactness_worst as string,",
				"          concavity_worst as string,",
				"          {concave points_worst} as string,",
				"          symmetry_worst as string,",
				"          fractal_dimension_worst as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     fileSystem: 'datalake',",
				"     folderPath: (concat($path, $file_name))) ~> source1",
				"source1 select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(in($id, id)) ~> filter1",
				"filter1 sort(asc(radius_mean, true)) ~> sort1",
				"sort1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}